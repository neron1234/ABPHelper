<UserControl x:Class="ABPHelper.AbpHelperWindowControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:views="clr-namespace:ABPHelper.Views"
             Background="{DynamicResource VsBrush.Window}"
             Foreground="{DynamicResource VsBrush.WindowText}"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300"
             Name="AbpHelper">
    <Grid>
        <TabControl>
            <TabItem Header="Generate Business">
                <views:AddNewBusiness/>
            </TabItem>
            <TabItem Header="Generate ApplicationService Methods">
                <views:AddNewServiceMethod/>
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>